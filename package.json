{
	"name": "cherry-components",
	"version": "0.0.2-20",
	"description": "Cherry React Components",
	"main": "dist/cherry.js",
	"module": "dist/cherry.module.js",
	"types": "@types/cherry.d.ts",
	"author": "Luan Gjokaj <luan@riangle.com>",
	"homepage": "https://cherry.design",
	"repository": "https://github.com/cherry-design-system/react-components",
	"license": "MIT",
	"keywords": [
		"cherry",
		"react",
		"components",
		"css",
		"framework",
		"design",
		"design system"
	],
	"scripts": {
		"build": "rollup -c rollup.config.js --bundleConfigAsCjs",
		"watch": "rollup -c rollup.config.js --bundleConfigAsCjs -w",
		"lint": "eslint src/**/*.js",
		"lint:fix": "eslint src/**/*.js --fix",
		"format": "prettier --write \"./src/**/*.{js,json,ts,tsx}\" \"!.node_modules/**\""
	},
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@emotion/babel-plugin": "^11.10.5",
		"@emotion/babel-preset-css-prop": "^11.10.0",
		"@emotion/eslint-plugin": "^11.10.0",
		"@rollup/plugin-babel": "^6.0.3",
		"babel-eslint": "^10.1.0",
		"eslint": "^8.32.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"prettier": "^2.8.3",
		"rollup": "^3.10.1",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-dts": "^5.1.1",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-replace": "^2.2.0",
		"rollup-plugin-terser": "^7.0.2"
	},
	"peerDependencies": {
		"@emotion/react": "^11.4.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
